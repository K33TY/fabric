package fabric.util;
import fabric.lang.security.Label;
import fabric.lang.Object;
import fabric.lang.JifObject;
import fabric.lang.JifObjectWrapper;

public class JifUtil {	
	
	public static Object unwrap(JifObject jobj) {
		if(jobj instanceof JifObjectWrapper) {
			return ((JifObjectWrapper)jobj).getObject();
		} else 
			return (Object)jobj;
	}
}
