package tree;

class TreeNode[label L] {
  TreeNode[L]{L} left, right;
  int{L} value;

  public TreeNode{L}(int{L} value) {
    this(null, null, value);
  }

  public TreeNode{L}(TreeNode[L]{L} left, TreeNode[L]{L} right, int{L} value) {
    this.left = left;
    this.right = right;
    this.value = value;
  }
}

