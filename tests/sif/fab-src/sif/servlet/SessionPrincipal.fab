package sif.servlet;

/**
 * A session principal. This principal represents an HTTP session.
 */
public final class SessionPrincipal[covariant label T] extends AbstractPrincipal {
    final SessionState[T]{T} session;
    public SessionPrincipal(SessionState[T]{T} session) {
	this.session = session;
        super("");
    }
    
    public static SessionPrincipal[{P←}]{P←} declassifySP{P←}(SessionPrincipal[{P→;P←}]{P→;P←} sp_, principal{P←} P)
    where caller(P) {
    	final SessionPrincipal[{P→;P←}]{P←} sp = declassify(sp_, {P→;P←} to {P←});
    	if(sp == null) return null;
    	final SessionState[{P←}]{P←} ss = SessionState.declassifySS(sp.session, P);
    	return new SessionPrincipal[{P←}](ss);
    }
    

}
