/**
 * A test harness for some of the classes in fabric.util.
 */

import fabric.util.*;
import fabric.client.*;

public class UtilTest {
  public static void main(String[] args) {
    Core core = Client.getClient().getCore("core0");
    Map map;
    List list;
    atomic {
      map = new HashMap@core();
      map.put("1", "6");
      map.put("8", "7");
      map.put("8", "1");
      map.put("0", "5");
      map.put("9", "2");
      map.put("7", "7");
      map.put("2", "5");
      map.put("3", "0");
      map.put("5", "2");
      map.put("6", "9");
      map.put("4", "8");
      map.put("2", "4");
      map.put("5", "3");
    }

    atomic {
      list = new LinkedList@core();
      list.add("3");
      list.add("8");
      list.add("9");
      list.add("5");
      list.add("2");
      list.add("0");
      list.add("1");
      list.add("7");
      list.add("4");
      list.add("6");
    }

    System.out.println("0 through 9 should follow.");
    Core local = Client.getClient().getLocalCore();
    for (Iterator it = list.iterator(local); it.hasNext(); ) {
      System.out.println(map.get(it.next()));
    }
  }
}

