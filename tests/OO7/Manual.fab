package OO7;

public class Manual {
  String title;
  long   id;
  String text;
  Module mod;

  public void setTitle(String title) {
    this.title = title;
  }

  public String title() {
    return title;
  }

  public void setId(long id) {
    this.id = id;
  }

  public long id() {
    return this.id;
  }

  public void setText(String text) {
    this.text = text;
  }

  public String text() {
    return text;
  }

  public void setModule(Module x) {
    mod = x;
  }

  public Module module() {
    return mod;
  }
}
