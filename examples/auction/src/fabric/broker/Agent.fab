package broker;

public interface Agent[label Airline, label Auction]
	where {Airline} <= {*provider}, {Auction} <= {*provider}, {Airline} <= {Auction}
{
    void prepareForAuction{Airline}();
	
    Offer[Auction] {Auction} makeFirstOffer {Airline} (); 
	
    Offer[Auction] {Auction} makeOffer {Auction} (UserAgent[Auction]{Auction} userChoice, 
		Offer[Auction]{Auction} bestOffer); 
}
