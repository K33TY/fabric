package user;
import broker.Offer;

public class UserAgent[label L] implements broker.UserAgent[L] 
	where {*broker.UserAgent.provider} <= {*provider}
{

    public int {L} choice{L}(Offer [L]{L} offer1, Offer[L]{L} offer2)  {  // return -1 if offer1 is as good or better than offer2
		if (offer1 == null  && offer2 == null) return 0;
		if (offer1 == null) return 1;
		if (offer2 == null) return -1; 

		if (offer1.price < offer2.price) return -1;
		if (offer2.price > offer2.price) return 1;
		return 0;
    }
}
