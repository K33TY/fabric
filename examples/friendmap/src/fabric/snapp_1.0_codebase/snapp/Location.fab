package snapp;

// TODO: document labels

/**
 * A point on the plane.
 *
 * @param L the update label
 * @param A the access label
 */
public class Location[label L, label A]
where
  L <= A // general constraint
{
  public float{L}@{A} x;
  public float{L}@{A} y;

  public
  Location
  {   // side effects:
    L // fields are updated
  }
  (
    int{L} x, int{L} y
  )
  {
    this.x = x;
    this.y = y;
  }
}

