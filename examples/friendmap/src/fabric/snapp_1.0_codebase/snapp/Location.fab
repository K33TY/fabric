package snapp;

/**
 * A point on the plane.
 */
public class Location[label A]
where provider <= A
{
  public final float{A}@{A} x;
  public final float{A}@{A} y;

  public
  Location{A}(int{A} x, int{A} y)
  {
    this.x = x;
    this.y = y;
  }
}

