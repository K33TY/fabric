package snapp;

// TODO: document labels

/**
 * A point on the plane.
 *
 * @param L the update label
 * @param A the access label
 */
public class Location[label L, label A]
where provider <= A
{
  public final float{A}@{A} x;
  public final float{A}@{A} y;

  public
  Location
  {      // side effects:
    A // fields are updated
  }
  (
    int{A} x, int{A} y
  )
  {
    this.x = x;
    this.y = y;
  }
}

