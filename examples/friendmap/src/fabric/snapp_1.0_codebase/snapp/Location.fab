package snapp;

/**
 * A point on the plane.
 */
public class Location[label AccessLabel] {
  public final float{this}@{AccessLabel} x;
  public final float{this}@{AccessLabel} y;

  public
  Location(int{this} x, int{this} y)
  where provider <= {this}
  {
    this.x = x;
    this.y = y;
  }
}

