public class Init {
  public static void main(String[] args) {
    final Store     store   = worker$.getRemoteStore("friendmap");
    final MapServer mapserv = (MapServer) worker$.getRemoteStore("mapserv").getRootMap().get("mapserv");

    final FriendMap app = new FriendMap(mapserv);
    store.getRootMap().put("friendmap", app);
  }
}



