package util;

import java.io.PrintStream;

/**
 * This class is here as a hack to work around shortcomings in the jif runtime.
 */

public final class System {
  public static final principal user = Worker;

  public static native
  PrintStream[lbl] {user←; user→}
  stdout
  (
    label lbl
  )
  where
    lbl <= {user→; _←_};
}


